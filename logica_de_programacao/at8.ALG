Algoritmo "idade"

Var
   aniversario, ano_atual, idade:inteiro
Inicio
      escreva("Em que ano você nasceu?")
      leia(aniversario)
      escreva("Qual é o ano atual?")
      leia(ano_atual)
      idade<-ano_atual-aniversario
      escreva("Você tem, aproximadamente,",idade," anos")

Fimalgoritmo