Algoritmo "at26"

Var
   cot, dolares:real
Inicio
      escreval("Informe quantos reais vale um dólar hoje:")
      leia(cot)
      escreval("Quantos dólares você possui?")
      leia(dolares)
      limpatela
      escreva("O valor que você possui, convertido em reais, é igual a R$",dolares*cot)
Fimalgoritmo